@page "/AgregarVehiculo"
@inject HttpClient Http
@inject NavigationManager Navigation

<FormularioVehiculo 
                    textoBoton="Agregar Vehiculo" 
                    textoTitulo="Agregar nuevo Vehiculo" 
                    metodo="@agregarVehiculo"
                    vehiculo="@vehiculo" 
                    />

@code {
        public Vehiculo vehiculo = new Vehiculo {estado = "Disponible"};

    async Task agregarVehiculo()
    {
        await Http.PostJsonAsync("Vehiculo", vehiculo);

        Navigation.NavigateTo("Vehiculo");
    }
}
